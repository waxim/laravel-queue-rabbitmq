{
  "name": "labour/laravel-queue-rabbitmq",
  "description": "RabbitMQ driver for Laravel Queue",
  "license": "MIT",
  "authors": [
    {
      "name": "Vladimir Yuldashev",
      "email": "misterio92@gmail.com"
    },
    {
      "name": "Alan Cole",
      "email": "alan_cole@labour.org.uk"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/database": "5.6.*",
    "illuminate/support": "5.6.*",
    "illuminate/queue": "5.6.*",
    "php-amqplib/php-amqplib": "^2.7",
    "queue-interop/queue-interop": "^0.6",
    "queue-interop/amqp-interop": "^0.7",
    "enqueue/amqp-tools": "^0.8.5",
    "enqueue/amqp-lib": "0.8.21",
    "queue-interop/amqp-interop": "^0.7"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "illuminate/events": "5.6.*",
    "mockery/mockery": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "VladimirYuldashev\\LaravelQueueRabbitMQ\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VladimirYuldashev\\LaravelQueueRabbitMQ\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "7.0-dev"
    },
    "laravel": {
      "providers": [
        "VladimirYuldashev\\LaravelQueueRabbitMQ\\LaravelQueueRabbitMQServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
